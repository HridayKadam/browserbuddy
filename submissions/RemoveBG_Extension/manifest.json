{
  "name": "CopyBG",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Extension that lets you copy images without the bg",
  "icons": {
    "48": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "action.js"
  },
  "permissions": [
    "contextMenus",
    "storage",
    "clipboardWrite",
    "activeTab",
    "notifications"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "eddiewird@gmail.com",
      "strict_min_version": "109.0"
    }
  },
  "default_locale": "en"
}
