{
    "manifest_version": 3,
    "name": "HighSeas Doubloons",
    "version": "1.0.0",
    "description": "A simple extension that adds some new features to the HighSeas-Shipyard page",
    "content_scripts": [
        {
            "matches": [
                "https://highseas.hackclub.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
      "16": "./icon-x48.png",
      "48": "./icon-x48.png",
      "128": "./icon-x48.png"
    },
    "action": {
      "default_icon": {
        "16": "./icon-x48.png",
        "48": "./icon-x48.png",
        "128": "./icon-x48.png"
      },
      "default_title": "HighSeas Doubloons Plus"
    }
}